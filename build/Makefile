# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=release
endif
export config

PROJECTS := applescript as-exec as-tell

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

applescript: 
	@echo "==== Building applescript ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f applescript.make

as-exec: applescript
	@echo "==== Building as-exec ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f as-exec.make

as-tell: applescript
	@echo "==== Building as-tell ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f as-tell.make

clean:
	@${MAKE} --no-print-directory -C . -f applescript.make clean
	@${MAKE} --no-print-directory -C . -f as-exec.make clean
	@${MAKE} --no-print-directory -C . -f as-tell.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   release"
	@echo "   debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   applescript"
	@echo "   as-exec"
	@echo "   as-tell"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
